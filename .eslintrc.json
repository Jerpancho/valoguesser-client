{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:prettier/recommended"
	],
	"overrides": [
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"prettier"
	],
	"rules": {
		"linebreak-style": [
			"error",
			"unix"
		],	
		"prettier/prettier": "error",
		"eqeqeq": ["error","always"]
	}
}
